LIB	=	../lib/libobjects.lib

OBJS	=	objects.obj transformations.obj

AR	=	lib
RM	=	del
CC	=	cl -Ox -I./ -I../ -DWIN32

ALL: $(LIB)

.SUFFIXES:	.obj

.c.obj:
	$(CC) $(CFLAGS) -c $<

$(LIB): $(OBJS)
	$(AR) -out:$(LIB) $(OBJS)

clean:
	$(RM) *.obj
