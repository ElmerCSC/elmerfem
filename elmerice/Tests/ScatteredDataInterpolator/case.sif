Solver 3 :: Reference Norm Tolerance = Real 1E-06
$fprintf( stderr, "TEST CASE 6\n");
Solver 3 :: Variable 1 = String "ZsCS"
RUN
$fprintf( stderr, "END TEST CASE 6: Target NRM=3.99996794E+02,EPS=1E-06\n" );
