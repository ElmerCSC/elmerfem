       r => BasisFunctions(n) % r
       Coeff => BasisFunctions(n) % Coeff

       s = 0.0d0
       DO i=1,BasisFunctions(n) % n
          s = s + Coeff(i)*u**p(i)*v**q(i)*w**r(i)
       END DO
       y(n) = s
