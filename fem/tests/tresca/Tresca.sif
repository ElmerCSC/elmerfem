Header
  Mesh DB "." "verkko.gid"
End

Constants
End

Simulation
  Max Output Level = 3
  Coordinate System = String Cartesian 2D
  Simulation Type = Steady State
  Steady State Min Iterations = 1
  Steady State Max Iterations = 1
!  Output File = File "esim1.result"
!  Post File = File "esim1.ep"
End

Body 1
  Equation  = 1
  Material = 1
End

Body Force 1
End

Material 1
  Density = Real 1
  Youngs Modulus = Real 1
  Poisson Ratio = Real 0.3
End

Equation 1
  Active Solvers(2) = 1 2
  Plane Stress = FALSE
End

Solver 1
  Equation = "Stresses"
  Procedure = "StressSolve" "StressSolver"

  Calculate Stresses = True
  Displace Mesh = False
  Linear System Solver = Direct
  Linear System Symmetric = True
  Linear System Scaling = True
  Linear System Direct Method = UMFPACK
  Linear System Iterative Method = BiCGStab
  Linear System Convergence Tolerance = 1.0e-8
  Linear System Max Iterations = 200
  Linear System Preconditioning = ILU2
  Nonlinear System Convergence Tolerance = 1.0e-8
  Nonlinear System Max Iterations = 1
  Nonlinear System Relaxation Factor = 1
  Steady State Convergence Tolerance= 1.0e-6
  Optimize Bandwidth = True
End

Solver 2
   Equation = "Tresca stress calculation"
   Procedure = "Tresca" "TrescaSolver"
End

! Bottom
!---------
Boundary Condition 1
  Target Boundaries(1) = 1
  Displacement 1 = Real 0
  Displacement 2 = Real 0
End

! Top
!------
Boundary Condition 2
  Target Boundaries(1) = 3
  Normal Force = Real 1.0
End

Solver 1 :: Reference Norm = Real 21.963995
Solver 1 :: Reference Norm = Real 21.963995
Solver 1 :: Reference Norm Tolerance = Real 1e-4
$fprintf( stderr, "TEST CASE 1\n");
RUN
$fprintf( stderr, "END TEST CASE 1: Target NRM=21.963995,EPS=1e-4\n" );
