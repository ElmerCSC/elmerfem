Header
  Mesh DB "." "Step3d"
  Include Path ""
  Results Directory ""
End


Simulation
  Max Output Level = 5
  Coordinate System =  Cartesian 3D
  Coordinate Mapping(3) = 1 2 3
  Simulation Type = Steady
  Steady State Max Iterations = 1
  Output Intervals = 1
!  Post File = "Step.ep"
End


Constants
  Gravity(4) = 0 -1 0 9.82
  Stefan Boltzmann = 5.67e-8
End


Body 1
  Name = "Body1"
  Equation = 1
  Material = 1
End

Equation 1
  Name = "Equation1"
  Active Solvers(2) = 1 2
  Ns convect = Logical False
End


Solver 1
  Equation = "Navier-Stokes"

  Linear System Solver = Iterative
  Linear System Iterative Method = BiCGStab
  Linear System Max Iterations = 500

  Stabilization Method = String Stabilized

  Linear System Convergence Tolerance = 1.0e-8
  Linear System Preconditioning = ILU1
  Linear System Residual Output = 10
  Steady State Convergence Tolerance = 1.0e-5

  Nonlinear System Convergence Tolerance = 1.0e-5
  Nonlinear System Max Iterations = 1
  Nonlinear System Newton After Iterations = 3
  Nonlinear System Newton After Tolerance = 1.0e-2
  Nonlinear System Relaxation Factor = 1.0
End


Solver 2
  Equation = "VortisitySolver"
  Procedure = "VorticitySolver" "VorticitySolver"
End


Material 1
  Name = "Material1"
  Density = 1
  Viscosity = 0.01
End

Boundary Condition 1
  Name = "inlet"

  Target Boundaries = 1

  Velocity 1 = Variable Coordinate 2, Coordinate 3
    Real MATC "4*(tx(0)-1)*(2-tx(0))*(1+tx(1))*(1-tx(1))"
  Velocity 2 = 0
  Velocity 3 = 0
End

Boundary Condition 2
  Name = "outlet"
  Target Boundaries = 2

  Velocity 2 = 0
  Velocity 3 = 0
End

Boundary Condition 3
  Name = "walls"
  Target Boundaries(2) = 3 5

  Velocity 1 = 0
  Velocity 2 = 0
  Velocity 3 = 0
End

Boundary Condition 4
  Name = "symmetry"
  Target Boundaries = 4

  Velocity 3 = 0
End

Solver 2 :: Reference Norm = Real 0.887172
Solver 2 :: Reference Norm Tolerance = Real 1e-2
RUN
