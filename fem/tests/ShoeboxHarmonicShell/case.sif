! This is test case is related to the Shoebox test cases for FSI.
!
! This case only tests the shell solver run alone as a body id
! AND that it can run in harmonic mode.
!
! P.R. 11.12.2017


$ AirDensity = 1.21
$ SoundSpeed = sqrt( 1.402 * 0.402 * 716 * 293)


Header
  Mesh DB "." "shoebox"
End

Constants
End

Simulation
  Max Output Level = 32
  Coordinate System = Cartesian
  Simulation Type = Steady State
  Output Intervals = 1
  Steady State Max Iterations = 1

  Post File = "shell.vtu"
  Frequency = 100
End

Body 1
  Equation = 1 
  Material = 1
End

Body 2
  Equation = 2
  Material = 2
  Body Force = 1
End


Body Force 1
  Normal Pressure = Real -1.0e5
End


Material 1
  Density = Real $ AirDensity
  Sound Speed = $ SoundSpeed
End

Material 2
  Density = 2710.0
  Youngs Modulus = 70e9
  Poisson Ratio = 0.3
  Shell Thickness = 0.001
End


Equation 1
  Name = "nothing here" 
End

Equation 2
  Name = "shell solver"
  Active Solvers(1) = 1
End


Solver 1
  Equation = "Shell equations"

  Procedure = "ShellSolver" "ShellSolver"

!  Linear System Solver = "Direct"
  Linear System Solver = "Iterative"
  Linear System Direct Method = MUMPS

  Linear System Preconditioning = ILU2
!  Linear System Row Equilibration = Logical True
  Linear System Max Iterations = 1000
  Linear System Convergence Tolerance = 1e-9
  Linear System Iterative Method = bicgstab2 !GCR
  Linear System GCR Restart = 100
  Linear System Abort Not Converged = False
  Linear System Residual Output = 1 

  Steady State Convergence Tolerance = 1e-09
  Nonlinear System Convergence Tolerance = 1e-8
  Nonlinear System Max Iterations = 1

  Harmonic Mode = Logical True

! The defaults are True for the following 2 keywords:
  Linear System Scaling = False
!  Optimize Bandwidth = False
  Linear System Complex = False
End


Solver 2
  Equation = "SaveScalars"
  Procedure = File "SaveData" "SaveScalars"

  Variable 1 = U 3
  Operator 1 = min 
  Operator 2 = max 
  Operator 3 = int mean

  Filename = shell.dat
End


Boundary Condition 1
  Target Boundaries(4) = 1 2 3 4
  Name = "Sides"

  U 1 = Real 0
  DNU 1 = Real 0
  U 2 = Real 0
  DNU 2 = Real 0
  U 3 = Real 0
  DNU 3 = Real 0
End 

Boundary Condition 2
  Target Boundaries(1) = 5
  Name = "Bottom"
End 

Boundary Condition 3
  Target Boundaries(1) = 6
  Name = "Fsi"

  Save Scalars = True
  Body Id = 2
End


Solver 1 :: reference norm = 2.54938680E+01