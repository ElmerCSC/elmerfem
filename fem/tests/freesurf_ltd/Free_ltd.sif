Header
  Mesh DB "." "Mesh"
End


Constants
  Gravity(4)=  0 1 0 9.81
  Stefan Boltzmann =  5.67e-8
End


Simulation
  Max Output Level = 5
  Coordinate System = Cartesian 2D
  Simulation Type ="Transient"
  Output Intervals = 1
  Steady State Max Iterations = 1
  Steady State Min Iterations = 1
  Timestepping Method = BDF
  BDF Order = 1
  Timestep Sizes = $31556926.0 * 0.025
  Timestep Intervals = 30
!   Post File   = "Free_ltd.ep"
 ! Output File = "Free_ltd.result"
End


Body 1
  Equation = 1
  Material = 1
  Body Force = 1
  Initial Condition = 2
End

Body 2
  Equation  = 2
  Material  = 1
  Body Force = 2
  Initial Condition = 2
End


Solver 1
  Equation = Navier-Stokes
  Stabilize = True
  Nonlinear System Max Iterations  = 1
  Nonlinear System Convergence Tolerance = 1.0e-6
  Nonlinear System Newton After Iterations = 8
  Nonlinear System Newton After Tolerance = 1.0e-3
  Nonlinear System Relaxation Factor = 0.9
  Linear System Solver = Direct
  Steady State Convergence Tolerance = 1.0e-4
End

Solver 2
  Equation = Free
  Variable = FreeSurface
  Procedure = "FreeSurfaceSolver" "FreeSurfaceSolver"
  Linear System Solver = Direct
  Linear System Convergence Tolerance = 1.0E-09
  Nonlinear System Max Iterations = 100
  Nonlinear System Min Iterations = 2
  Nonlinear System Convergence Tolerance = 1.0e-08
  Steady State Convergence Tolerance = 1.0e-6
  Exported Variable 1 = Freesurface Residual
  Exported Variable 1 DOFs = 1
  Stabilization Method = Stabilized
  Apply Dirichlet = Logical True
End

Solver 3
  Equation = Mesh Update
  Linear System Solver = Direct
  Steady State Convergence Tolerance = 1.0e-6
End


Equation 1
  Active Solvers(2) = 1 3
End


Equation 2
  Active Solvers(1) = 2
  Convection = "computed"
  Flow Solution Name = String "Flow Solution"
End

Body Force 1
  Flow BodyForce 1 = 0
  Flow BodyForce 2 = -9.81
End

Body Force 2
   FreeSurface Accumulation Flux 1 = Real 0.0
   FreeSurface Accumulation Flux 2 = Variable Coordinate 1
      Real MATC "1.0*(4.0 - sqrt((tx-5.0)*(tx-5.0)))/(31556926.0)"
End

Material 1
  Viscosity = 1.0E12
  Density = 918.0
  Youngs Modulus = 1
  Poisson Ratio = 0.3
  Min FreeSurface = Real 0.0
  Max FreeSurface = Real 1.4
End

Initial Condition 1
  Velocity 1 = 0.0E00
  Velocity 2 = 0.0E00
  Pressure = 0.0
  Mesh Update 1 = 0.0
  Mesh Update 2 = 0.0
  FreeSurface = Real 0.4
End

Initial Condition 2
  FreeSurface = Real 0.4
End

Boundary Condition 1
  Target Boundaries = 1
  Name = "Bed"
  Velocity 1 = 0
  Velocity 2 = 0
  Mesh Update 1 = 0
  Mesh Update 2 = 0
End


Boundary Condition 2
  Target Boundaries = 3
  Name = "Free"
  Body Id = 2
  Flow Force BC = True
  Surface Tension Coefficient = 0
  Mesh Update 2 = Variable FreeSurface
     Real MATC "tx - 0.4"
  Mesh Update 1 = 0
End


Boundary Condition 3
  Target Boundaries(2) = 2 4
  Name = "Outflow" 
  Mesh Update 1 = 0
End



Solver 3 :: Reference Norm = Real 0.334858281808
RUN
