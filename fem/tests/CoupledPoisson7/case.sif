!# Solves two decoupled Poisson equations at same time using coupling at the interface
!# This way two different equation may be joined at a interface.

Header
  CHECK KEYWORDS Warn
  Mesh DB "." "angle"
  Include Path ""
  Results Directory ""
End

Simulation
!  Max Output Level = 4
  Coordinate System = "Cartesian"
  Coordinate Mapping(3) = 1 2 3

  Simulation Type = "Steady State"
  Steady State Max Iterations = 1
  Output Intervals = 1

!  Post File = "case.ep"
End

Constants
  Gravity(4) = 0 -1 0 9.82
  Stefan Boltzmann = 5.67e-08
End

Body 1
  Name = "Body1"

  Body Force = 1
  Equation = 1
  Material = 1
End

Body 2
  Name = "Body2"

  Body Force = 1
  Equation = 2
  Material = 1
End

Equation 1
  Name = "Equation1"
  Active Variables(1) = Integer 1 
End

Equation 2
  Name = "Equation2"
  Active Variables(1) = Integer 2
End


Solver 1
  Coupled Solver = Logical True

  Steady State Convergence Tolerance = 1.0e-05
  Nonlinear System Relaxation Factor = Real 1.0
  Nonlinear System Max Iterations = Integer 3

  Linear System Solver = "Iterative"
  Linear System Iterative Method = bicgstab
  Linear System Convergence Tolerance = 1.0e-8
  Linear System Max Iterations = 500
  Linear System Preconditioning = ILU0

  Equation = "test"
  Variable 1 = String T1
  Bulk Assembly Procedure 11 = File "./PoissonAssembly" "BulkAssembly"
  Variable 2 = String T2
  Bulk Assembly Procedure 22 = File "./PoissonAssembly" "BulkAssembly"

  Constraint 1 = String "consa"
  Constraint 1 Type = String "equality"
  Constraint 1 Variables(2) = Integer 1 2
  Constraint 1 Value = Real 0.0
End  


Solver 2
  Exec Solver = never

  Procedure = "SaveData" "SaveScalars"
  Filename = f.dat
  Variable 1 = T1
  Operator 1 = min
  Variable 2 = T1
  Operator 2 = max
  Variable 3 = T2
  Operator 3 = min
  Variable 4 = T2
  Operator 4 = max
End 



Material 1
  Name = "Material1"

  Density = real 1
  Conductivity = Real 1.0
  Reaction = Real 0.0
  Source = Real 1.0
End

Body Force 1
  Name = "BodyForce1"
  Heat Source = real 1
End

Boundary Condition 1
  Target Boundaries(1) = 1

  T1 = Real 0
  T2 = Real 0
End

Boundary Condition 2
  Target Boundaries(1) = 2 

  T1 = Real 0
  T2 = Real 0
End

Boundary Condition 3
  Target Boundaries(1) = 3 

  consa = Logical True
End

Solver 1 :: Reference Norm = Real 0.78409170E-01
RUN


