Check Keywords "Warn"

Header
  Mesh DB "." "Step"
End

Simulation
  Max Output Level = 6
  Coordinate System = "Cartesian 2D"
  Simulation Type = Steady
  Output Intervals(1) = 1
  Steady State Max Iterations = 1
!  Post File   = "ptest.ep"

  Set Dirichlet BCs By BC Numbering = True
! Output File = "ptest.dat"
End

Body 1
  Equation = 1
  Material = 1
End

Equation 1
  Active Solvers(1) = 1
End

Material 1
  Density   = 1
  Viscosity = 1
End
 
Solver 1
  Equation = "Stokes"
  Variable = "Stokes"

  Element = "p:2"

  Variable DOFs = 3
  Procedure = "Stokes1" "StokesSolver"

  Nonlinear System Relaxation Factor = 1

  Linear System Solver = "Iterative"
  Linear System Iterative Method = BiCGStab
  Linear System Max Iterations = 100
  Linear System Convergence Tolerance = 1.0e-6
  Linear System Preconditioning = ILU1
  Steady State Convergence Tolerance = 1e-05
  Before Linsolve = "EliminateDirichlet" "EliminateDirichlet"
End

Boundary Condition 1
  Target Boundaries(1) = 1
  Stokes 1 = Real 1
  Stokes 2 = Real 0
End

Boundary Condition 2
  Target Boundaries(1) = 2
  Stokes 2 = Real 0
End

Boundary Condition 3
  Target Boundaries(1) = 3
  Stokes 1 = Real 0
  Stokes 2 = Real 0
End

Solver 1 :: Reference Norm = Real 5.4126327
Solver 1 :: Reference Norm Tolerance = Real 1e-3
RUN

