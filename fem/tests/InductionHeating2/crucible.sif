Check Keywords Warn

Header
  Mesh DB "." "crucible"
  Include Path ""
  Results Directory ""
End

Simulation
  Max Output Level = 5
  Coordinate System = "Axi Symmetric"
  Simulation Type = Steady State
  Steady State Max Iterations = 1
!  Post File = "crucible.ep"
End

Constants
  Permittivity Of Vacuum = 8.8542e-12
  Stefan Boltzmann = 5.67e-8
End

Body Force 1
  Joule Heat = Logical True
End

Body Force 2
  Current Density = 1.0e6
End


Body 1
  Name = "Air_outside"
  Equation = 1
  Material = 1
End

Body 2
  Name = "Air_inside"
  Equation = 2
  Material = 1
  Body Force = 1
End

Body 3
  Name = "Insulation"
  Equation = 2
  Material = 2
  Body Force = 1
End

Body 4
  Name = "Graphite"
  Equation = 2
  Material = 3
  Body Force = 1
End

Body 5
  Name = "Powder"
  Equation = 2
  Material = 4
  Body Force = 1
End

Body 6
  Name = "Coil"
  Equation = 1
  Material = 1
  Body Force = 2
End


Equation 1
  Name = "Just induction"
  Active Solvers = 1 
End

Equation 2
  Name = "Induction and heating"
  Active Solvers(2) = 1 2
End

Solver 1
  Equation = Potential Solver

  Procedure = "StatMagSolve" "StatMagSolver"
  Variable = Potential
  Variable DOFs = 2

  Frequency = Real 50.0e3
  Calculate Joule Heating = Logical True
  Calculate Magnetic Flux = Logical False
  Calculate Magnetic Flux Abs = Logical False

  Desired Heating Power = Real 3.0e3

  Linear System Solver = Direct
  Linear System Direct Method = umfpack
  Nonlinear System Max Iterations = 1
  Nonlinear System Convergence Tolerance = 1.0e-6
  Nonlinear System Relaxation Factor = 1
End


Solver 2
  Equation = heat equation
!solver
  Variable = Temperature
  Procedure = "HeatSolve" "HeatSolver"

  Radiation Solver = Logical True
  Steady State Convergence Tolerance = 1.0e-5

  Nonlinear System Convergence Tolerance = 1.0e-4
  Nonlinear System Max Iterations = 20
  Nonlinear System Newton After Iterations = 3
  Nonlinear System Newton After Tolerance = 1.0e-5
  Nonlinear System Relaxation Factor = 1
  Nonlinear System Convergence Measure = solution   

  Linear System Solver = Iterative
  Linear System Iterative Method = BiCGStab
  Linear System Max Iterations = 500
  Linear System Convergence Tolerance = 1.0e-8
  Linear System Preconditioning = ILU1
  Linear System ILUT Tolerance = 1.0e-3
  Linear System Abort Not Converged = False
  Linear System Residual Output = 10
  Linear System Precondition Recompute = 1

  Minimum Gebhardt Factor = Real 1.0e-10
End 

Solver 3
  Exec Solver = never

  Equation = SaveFields
  Procedure = "ResultOutputSolve" "ResultOutputSolver"
  ElmerPost Format = Logical True
  Output File Name = "crucible"
  Mask Variable = Temperature
End 


Material 1
  Name = "Air"
  Electric Conductivity = 0.0
  Heat Conductivity = 0.024
  Density = 1.3
End

Material 2
  Name = "Insulation"
  Electric Conductivity = 2.0E3
  Heat Conductivity = 1.0
  Density = 500.0
  Emissivity = 0.9
End

Material 3
  Name = "Graphite"
  Electric Conductivity = 2.0E4
  Heat Conductivity = 50.0
  Density = 1000.0
  Emissivity = 0.9
End

Material 4
  Name = "Powder"
  Electric Conductivity = 1.0E4
  Density = 2000.0
  Heat Conductivity = 5.0
  Emissivity = 0.7
End

Boundary Condition 1
  Name = "Outside_bc"
  Target Boundaries = 1
  Potential 1 = Real 0.0
  Potential 2 = Real 0.0
End

Boundary Condition 2
  Name = "Water_cooling"
  Target Boundaries = 2
  Temperature = 300
End

Boundary Condition 3
  Name = "Radiation_bc"
  Target Boundaries = 3
  Heat Flux BC = True
  Radiation = Diffuse Gray
End







Solver 2 :: Reference Norm = Real 1213.1768944333069
RUN

