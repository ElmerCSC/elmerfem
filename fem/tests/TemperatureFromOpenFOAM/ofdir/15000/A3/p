FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "15000/A3";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    back
    {
        type            wedge;
    }
    front
    {
        type            wedge;
    }
    A3_to_fluid
    {
        type            calculated;
        value           uniform 0;
    }
}


