$ c = 340

Header
  CHECK KEYWORDS Warn
  Mesh DB "." "angle_in_halfcircle"
  Include Path ""
  Results Directory ""
End

Simulation
  Max Output Level = 12

  Coordinate System = "Cartesian 2D"
  Coordinate Mapping(3) = 1 2 3

  Simulation Type = "Steady State"
  Steady State Max Iterations = 1
  Output Intervals = 1

! Post File = "case.vtu"
End

Constants
  Gravity(4) = 0 -1 0 9.82
  Stefan Boltzmann = 5.67e-08
End

Body 1
  Name = "fluid"
  Equation = 1
  Material = 1
End

Body 2
  Name = "structure"
  Equation = 2
  Material = 2
End

Equation 1
  Active Solvers(1) = 2
End

Equation 2
  Active Solvers(1) = 1
End


Solver 1
  Equation = "Stress Analysis"
  Variable = "Displacement"
  Variable Dofs = 2
  Plane Stress = Logical True

  Eigen Analysis = True
  Eigen System Values = 5
  Eigen System Convergence Tolerance = Real 1.0e-6

  Linear System Scaling = Logical True
  Linear System Symmetric = Logical True

  Linear System Solver = Direct
  Linear System Direct Method = UMFPack
  Linear System Convergence Tolerance = 1.0e-6
  Linear System Iterative Method = Bicgstab
  Linear System Max Iterations = 500
  Linear System Preconditioning = ILU1
  Linear System Residual Output = 0
  Linear System Abort Not Converged = True

  Nonlinear System Convergence Tolerance = 1.0e-05
  Nonlinear System Max Iterations = 1
  Nonlinear System Newton After Iterations = 3
  Nonlinear System Newton After Tolerance = 1.0e-02
  Nonlinear System Relaxation Factor = 1.0
End



Solver 2
  Equation = "Helmholtz"
  Variable = "Pressure"
  Variable Dofs = 2

  Procedure = "HelmholtzSolve" "HelmholtzSolver" 

  Linear System Solver = Direct

  Linear System Direct Method = umfpack

  Displacement Variable EigenMode = Integer 3
  Displacement Variable Frequency = Logical True

  Linear System Iterative Method = "BiCGStab"
  Linear System Max Iterations = 300
  Linear System Convergence Tolerance = 1.0e-08
  Linear System Abort Not Converged = True
  Linear System Preconditioning = "ILU2"
  Linear System Residual Output = 20
  Linear System Precondition Recompute = 1

  Steady State Convergence Tolerance = 1.0e-05

  Nonlinear System Convergence Tolerance = 1.0e-05
  Nonlinear System Max Iterations = 1
  Nonlinear System Newton After Iterations = 3
  Nonlinear System Newton After Tolerance = 1.0e-02
  Nonlinear System Relaxation Factor = 1
End

Solver 3
  Exec Solver = never

  Equation = "SaveScalars"
  Filename = f.dat

  Procedure = "SaveData" "SaveScalars"
  Variable 1 = Pressure 1
  Operator 1 = max abs 
  Variable 2 = Pressure 2
  Operator 2 = max abs 

  Variable 3 = Displacement 1
  Operator 3 = max abs 
  Variable 4 = Displacement 2
  Operator 4 = max abs 
End


Material 1
  Name = "Fluid"

  Density = 1.224
  Sound Speed = $ c
End

Material 2
  Name = "Structure"
  Density = 1000.0
  Youngs Modulus = 1.0e10
  Poisson Ratio = 0.3
End


Boundary Condition 1
  Name = "outlet"
  Target Boundaries(2) = 1 2
  Plane Wave BC = True
End

Boundary Condition 2
  Name = "interface"
  Target Boundaries(5) = 3 4 5 6 7
!  Wave Flux 1 = 1 
  Structure Interface = Logical True
End 

Boundary Condition 3
  Name = "fluid_wall"
  Target Boundaries(2) = 8 9
End 


Boundary Condition 4
  Name = "structure_wall"
  Target Boundaries(1) = 10
  Displacement 1 = 0.0
  Displacement 2 = 0.0
End 

$fprintf( stderr, "TEST CASE 1\n");
RUN
$fprintf( stderr, "END TEST CASE 1: Target NRM=23152.284\n");

$fprintf( stderr, "TEST CASE 2\n");
Material 1 :: Convection Velocity 1 = $ 0.2*c
Material 1 :: Convection Velocity 2 = $ 0.1*c
RUN
$fprintf( stderr, "END TEST CASE 2: Target NRM=28132.083\n");
