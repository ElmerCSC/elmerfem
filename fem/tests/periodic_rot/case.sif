Header
  CHECK KEYWORDS Warn
  Mesh DB "." "."
  Include Path ""
  Results Directory ""
End

Simulation
  Max Output Level = 9
  Coordinate System = Cartesian
  Coordinate Mapping(3) = 1 2 3
  Simulation Type = Steady state
  Steady State Max Iterations = 1
  Output Intervals = 1
!  Post File = case.ep
  Solver Input File = case.sif

  Set Dirichlet BCs by BC Numbering = True
End

Constants
  Gravity(4) = 0 -1 0 9.82
  Stefan Boltzmann = 5.67e-08
  Unit Charge = 1.602e-19
  Boltzmann Constant = 1.3807e-23
  Permittivity of Vacuum = 8.8542e-12
End

Body 1
  Target Bodies(1) = 1
  Name = "Body 1"
  Equation = 1
  Material = 1
End

Solver 1
  Equation = Navier-Stokes

  Procedure = "FlowSolve" "FlowSolver"
  Variable = Flow Solution[Velocity:2 Pressure:1]

  Stabilize = True
  Steady State Convergence Tolerance = 1.0e-5

  Nonlinear System Convergence Tolerance = 1.0e-8
  Nonlinear System Max Iterations = 1
  Nonlinear System Newton After Iterations = 3
  Nonlinear System Newton After Tolerance = 1.0e-3
  Nonlinear System Relaxation Factor = 1

  Linear System Solver = Direct
  Linear System Direct Method = UMFPack
  Linear System Iterative Method = BiCGStab
  Linear System Max Iterations = 500
  Linear System Convergence Tolerance = 1.0e-8
  Linear System Preconditioning = ILU0
  Linear System ILUT Tolerance = 1.0e-3
  Linear System Abort Not Converged = False
  Linear System Residual Output = 1
  Linear System Precondition Recompute = 1
End

Equation 1
  Name = "Equation 1"
  Active Solvers(1) = 1
End

Material 1
  Name = "Material 1"
  Density = 1
  Viscosity = 0.1
End

Boundary Condition 1
  Target Boundaries(1) = 2
  Name = "wall"
  Velocity 1 = 0
  Velocity 2 = 0
End

Boundary Condition 2
  Target Boundaries(1) = 1
  Name = "outflow"

  Normal-Tangential Velocity = True
  Periodic BC Rotate(3) = Real  0 0 -90
  Periodic BC = Integer 3
  Periodic BC Velocity 1 = Logical True
  Periodic BC Velocity 2 = Logical True
! Periodic BC Use Lagrange Coefficient = Logical True
End


Boundary Condition 3
  Target Boundaries(1) = 6
  Name = "inflow"

  Pressure 1 = Variable Coordinate 1
     Real MATC "sq2=sqrt(2);t=2*(tx-sq2)*sq2-1; if (t<0) { 1 } else { 0 }"
  Velocity 2 =  0
  Normal-Tangential Velocity = True
End

Boundary Condition 4
  Target Coordinates(1,3) = 1.5 1 0
  Pressure = 0
End

Solver 1 :: Reference Norm = Real 0.13097278
$fprintf( stderr, "TEST CASE 1\n");
RUN
$fprintf( stderr, "END TEST CASE 1: Target NRM=0.13097278\n" );
