! Test for the simple mesh exctrusion within ElmerSolver
! Compared to the standard test this includes
! a division given by a MATC function.

Check Keywords "Warn"

Header
  Mesh DB "." "square"
End

Simulation

  Extruded Mesh Levels = Integer 20
!  Extruded Mesh Ratio = Real 0.25
  Extruded Mesh Density = Variable Coordinate 1
    Real MATC "1+10*tx"

!  Extruded Mesh Density = Variable Coordinate 1
!    Real
!      0.0 1.0
!      0.5 1.0 
!      1.0 4.0 
!    End 
  
  Max Output Level = 9
  Coordinate System = Cartesian
  Simulation Type = Steady
  Output Intervals(1) = 1
  Steady State Max Iterations = 1
! Post File   = "case.ep"
! Output File = "case.dat"
End

Body 1
  Equation = 1
  Body Force = 1
  Material = 1
End

Body Force 1 :: Heat Source = Real 1
Equation 1 :: Active Solvers(1) = 1


Material 1
  Name = "Material1"
  Density = 1
  Heat Conductivity = 1
End


Solver 1
  Equation = "Heat Equation"
  Variable = "Temperature"
  Variable Dofs = 1
  Linear System Solver = "Iterative"
  Linear System Iterative Method = "BiCGStabl"
  BiCgStabl Polynomial Degree = 4
  Linear System Max Iterations = 500
  Linear System Convergence Tolerance = 1.0e-08
  Linear System Abort Not Converged = True
  Linear System Preconditioning = "none"
  Linear System Residual Output = 1
  Steady State Convergence Tolerance = 1.0e-05
  Nonlinear System Convergence Tolerance = 1.0e-05
  Nonlinear System Max Iterations = 1
  Nonlinear System Newton After Iterations = 3
  Nonlinear System Newton After Tolerance = 1.0e-02
  Nonlinear System Relaxation Factor = 1.0
End


Boundary Condition 1
  Name = "old down and up"
  Target Boundaries(2) = 1 3
  Temperature = Real 0
End

Boundary Condition 2
  Name = "old right and left"
  Target Boundaries(2) = 2 4
!  Temperature = Real 0
End

Boundary Condition 3
  Name = "new top"
  Temperature = Real 0
End

Boundary Condition 4
  Name = "new bot"
  Temperature = Real 0
End

Solver 1 :: Reference Norm = Real 3.44528391581298971E-002
$fprintf( stderr, "TEST CASE 1\n");
RUN
$fprintf( stderr, "END TEST CASE 1: Target NRM= 3.44528391581298971E-002\n");

