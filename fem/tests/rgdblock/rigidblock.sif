Header
  Mesh DB "." "anturi"
End

Simulation
! Max Output Level = 3
  Coordinate System = Cartesian 3D
  Simulation Type = Steady State
  Steady State Max Iterations = 1
  Output Intervals = 1
! Output File = "anturi.dat"
! Post File = "anturi.ep"
End

Body 1
  Equation = 1
  Material = 2
!  Body Force = 1
End

Body 2
  Equation = 1
  Material = 1
!  Body Force = 1
End

Body 3
  Equation = 1
  Material = 1
  Body Force = 1
  Rigid Body = True
End

Equation 1
  Active Solvers = 1
End

Solver 1
  Equation = Stress Analysis
  Variable = Displacement
  Variable DOFs = 3
  Before Linsolve = "RigidBodyReduction" "RigidBody"
  Extend Elastic Region = Logical True
  Extend Elastic Layers = Integer 2
  Optimize Matrix Structure = Logical True
  Reverse Ordering = Logical True
  Output Node Types = Logical True

  Steady State Convergence Tolerance = 1.0e-05
  Linear System Solver =  Iterative
  Linear System Iterative Method = CG
  Linear System Max Iterations = 1000
  Linear System Residual Output = 10
  Linear System Convergence Tolerance = 1.0e-8
  Linear System Preconditioning = ILU4
  Linear System ILUT Tolerance = 1.0e-03
  Nonlinear System Convergence Tolerance = 1.0e-05
  Nonlinear System Max Iterations = 1
  Nonlinear System Newton After Iterations = 8
  Nonlinear System Newton After Tolerance = 1.0e-05
  Nonlinear System Relaxation Factor = 1.0
End

Material 1   
  Name = "Silicon"
  Youngs Modulus = 162.0e9
  Poisson ratio = 0.27
  Density = 2330.0
End

Material 2
  Name = "Weaker Silicon"
  Youngs Modulus = 100.0e9
  Poisson ratio = 0.27
  Density = 2330.0
End

Body Force 1

  Stress Bodyforce 2 = -1e6
End

Boundary Condition 1
  Target Boundaries(2) = 1 2
  Displacement 1 = 0.0
  Displacement 2 = 0.0
  Displacement 3 = 0.0
End

Solver 1 :: Reference Norm = Real 0.958531124454E-06
RUN
