Material 1
  Name = "Material1"
  Density = 1
  Viscosity = 1
End

Boundary Condition 1
  Name = "inflow"
  Target Boundaries = 1

  RotSol 1 = Variable Coordinate 2
     Real MATC "(2-tx)*(tx-1)"
  RotSol 2 = Real 0
End

Boundary Condition 2
  Name = "outflow"
  Target Boundaries = 2
  Outflow Boundary = Logical True
  RotSol 2 = Real 0
End

Boundary Condition 3
  Name = "wall"
  Target Boundaries = 3

  RotSol 1 = Real 0
  RotSol 2 = Real 0
End

Solver 1 :: Reference Norm = Real 1.2047758
RUN
