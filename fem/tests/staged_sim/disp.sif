!echo on
Check Keywords Warn

Header
  Mesh DB "." "mesh"
End

Simulation
  Coordinate System = Cartesian 2D
  Simulation Type = Steady

  Output Intervals = 1
  Steady State Min Iterations = 11
  Steady State Max Iterations = 11

  Output Version Numbers = Logical True
  Output File = disp.res
  Post File = disp.ep
End

Body 1
  Equation = 1
  Material = 1
End

Body 2
  Equation = 1
  Material = 2
  Body Force = 1
End


Equation 1
  Active Solvers(2) = 1 2
End


Solver 1
  Equation = heat equation

  Linear System Solver = Iterative
  Linear System Iterative Method = BiCGStab
  Linear System Preconditioning = ILU1
  Linear System Max Iterations = 300
  Linear System Convergence Tolerance = 1.0e-6

  Nonlinear System Max Iterations = 1
  Nonlinear System Convergence Tolerance = 1.0e-5

  Steady State Convergence Tolerance = 1.0e-5
End

Solver 2
  Equation = thermal stress
  Procedure = "StressSolve" "StressSolver"
  Variable = Displacement
  Variable DOFs = 2

  Exported Variable 1 = "Reference Displacement"
  Exported Variable 1 DOFs = 2

!  Optimize Bandwidth = False

  Linear System Solver = Iterative
  Linear System Iterative Method = BiCGStab
  Linear System Preconditioning = ILU1
  Linear System Max Iterations = 300
  Linear System Convergence Tolerance = 1.0e-6

  Nonlinear System Max Iterations = 1
  Nonlinear System Convergence Tolerance = 1.0e-5

  Steady State Convergence Tolerance = 1.0e-5
End


Material 1
  Density = 1
  Heat capacity = 1
  Heat Conductivity = 1000

  Youngs modulus = 1e9
  Poisson Ratio = 0.29

  Heat Expansion Coefficient = 1.0e-4
  Reference Temperature = 0
End


Material 2
  Density = 1
  Heat capacity = 1
  Heat Conductivity = 1000

  Youngs modulus = Variable Time
    Real
       0.0    1.0
       5.0    1.0
       5.1    1e9
       10.0   1e9
     End

  Poisson Ratio = 0.29

  Heat Expansion Coefficient = 1.0e-4
  Reference Temperature = Variable Time
    Real
       0.0     0.0
       5.0    100.0
       10.0   100.0
     End

End


Body Force 1

    Update Reference Displacement = Variable Time
     Real
      0.0    1.0
      5.0    1.0
      5.1   -3.0
      10.0  -3.0
     End   

End


Boundary Condition 1
  Target Coordinates(1,2) = Real 2.5  1.0

  Displacement 1 = 0.0
  Displacement 2 = 0.0

  Displacement 1 Condition = Variable Time
     Real
      0.0    1.0
      5.0    1.0
      5.1   -3.0
      10.0  -3.0
     End   

  Displacement 2 Condition = Variable Time
     Real
      0.0    1.0
      5.0    1.0
      5.1   -3.0
      10.0  -3.0
     End   

End


Boundary Condition 2
  Target Coordinates(1,2) = Real 2.5  0.0

  Displacement 1 = 0.0

  Displacement 1 Condition = Variable Time
     Real
      0.0    1.0
      5.0    1.0
      5.1   -3.0
      10.0  -3.0
     End   

End

Boundary Condition 3
  Target Boundaries = 1 

  Temperature = Variable Time
    Real
      0.0     0.0
      5.0   100.0
      6.0   100.0
     11.0     0.0
    End

End

Boundary Condition 4
  Target Boundaries = 2

  Temperature = Variable Time
    Real
      0.0     0.0
      5.0   100.0
      6.0   100.0
     11.0     0.0
    End

  Displacement 1 = 0.0
  Displacement 2 = 0.0
End

Solver 2 :: Reference Norm = Real 0.672410258E-02
Solver 2 :: Reference Norm Tolerance = Real 1.0e-3
RUN

! End of File
