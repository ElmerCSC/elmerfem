Check Keywords "Warn"

Header
  Mesh DB "." "Step"
End

Simulation
  Max Output Level = 5
  Coordinate System = "Cartesian"
  Simulation Type = Steady
  Output Intervals(1) = 1
  Steady State Max Iterations = 100
  Post File   = "ptest.ep"

! Set Dirichlet BCs By BC Numbering = True
! Output File = "ptest.dat"
End

Body 1
  Equation = 1
  Material = 1
End

Equation 1
  Active Solvers(1) = 1
  Element = "b:1"
End

Material 1
  Density   = 1
  Viscosity = 0.05
End
 
Solver 1
  Equation = "Stokes"
  Variable = Q1Q0[Velocity:2]
  Procedure = "Q1Q0" "StokesSolver"

  Exported Variable 1 = aPressure

  Nonlinear System Max Iterations = 1

  Bubbles In Global System = False
  Linear System Solver = "Iterative"

  Linear System Solver = direct
  Linear System Direct Method = umfpack

  Steady State Convergence Tolerance = 1e-05
  Before Linsolve = "EliminateDirichlet" "EliminateDirichlet"
End

Boundary Condition 1
  Target Boundaries(1) = 1
  Velocity 1 = Real 1
  Velocity 2 = Real 0
End

Boundary Condition 2
  Target Boundaries(1) = 2
  Velocity 2 = Real 0
End

Boundary Condition 3
  Target Boundaries(1) = 3
  Velocity 1 = Real 0
  Velocity 2 = Real 0
End

Solver 1 :: Reference Norm = Real 0.39106727
RUN
