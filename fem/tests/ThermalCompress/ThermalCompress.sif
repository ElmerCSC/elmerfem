Header
  Mesh DB "." "ThermalCompress"
End


Simulation
  Max Output Level = 3
  Coordinate System = Cartesian 2D
  Simulation Type = Steady
  Steady State Max Iterations  = 50
  Output Intervals = 1
! Post File =  "ThermalCompress.ep"
! Output File = "ThermalCompress.result"
End


Body 1
  Equation = 1
  Material = 1
  Body Force  = 1
  Initial Condition = 1
End

$ RefT=293

Initial Condition 1
  Temperature = $RefT
End

Body Force 1
  Flow BodyForce 2 = -10
End


Solver 1
  Equation = Heat Equation
  Stabilize = True
  Nonlinear System Max Iterations = 1
  Nonlinear System Convergence Tolerance = 1.0e-6
  Nonlinear System Newton After Iterations = 20
  Nonlinear System Newton After Tolerance = 1.0e-3
  Nonlinear System Relaxation Factor = 0.75
  Linear System Solver = Direct
  Linear System Iterative Method = UMFPack
  Steady State Convergence Tolerance = 1.0e-8
End


Solver 2
  Equation = Navier-Stokes
  Stabilize = True
  Nonlinear System Max Iterations = 1
  Nonlinear System Convergence Tolerance = 1.0e-6
  Nonlinear System Newton After Iterations = 20
  Nonlinear System Newton After Tolerance = 1.0e-3
  Nonlinear System Relaxation Factor = 1
  Linear System Solver = Direct
  Linear System Direct Method = UMFPack
  Steady State Convergence Tolerance = 1.0e-5
End


Equation 1
  Navier-Stokes = True
  Heat Equation = True
  Convection = Computed
End

Material 1
  Density = 1
  Viscosity = 1
  Heat Capacity = 1000
  Heat Conductivity  = 1.36986
  Reference Temperature = $RefT
  Compressibility Model = Thermal
  Heat Expansion Coefficient = 0.25
End


Boundary Condition 1
  Target Boundaries = 1

pressure  = 0
  Velocity 1 = 0
  Velocity 2 = 0
End


Boundary Condition 2
  Target Boundaries = 2

  Velocity 1 = 0
  Velocity 2 = 0
  Temperature = $RefT+0.5
End


Boundary Condition 3
  Target Boundaries = 3

  Velocity 1 = 0
  Velocity 2 = 0
  Temperature = Variable Coordinate 1
    Real MATC "RefT-tx+0.5"
End


Boundary Condition 4
  Target Boundaries = 4


  Velocity 1 = 0
  Velocity 2 = 0
  Temperature = $RefT-0.5
End

Solver 2 :: Reference Norm = Real 0.00331988
Solver 2 :: Reference Norm Tolerance = Real 1.0e-3
$fprintf( stderr, "TEST CASE 1\n");
RUN
$fprintf( stderr, "END TEST CASE 1: Target NRM=0.00331988,EPS=1.0e-3\n");
